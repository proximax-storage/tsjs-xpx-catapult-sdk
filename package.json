{
  "name": "proximax-nem2-sdk",
  "version": "0.10.5",
  "description": "Proximax Nem2 sdk for typescript and javascript",
  "scripts": {
    "pretest": "npm run build",
    "test": "mocha --ui bdd --recursive ./dist/test --timeout 90000",
    "test:e2e": "mocha --ui bdd --recursive ./dist/e2e --timeout 90000",
    "test:all": "mocha --ui bdd --recursive ./dist/ --timeout 90000",
    "build": "rm -rf dist/ && tsc",
    "test:cov": "nyc --reporter=lcov --reporter=text-lcov npm t && nyc report --reporter=text-lcov",
    "test:coveralls": "npm run test:cov | coveralls"
  },
  "contributors": [
    "Aleix <aleix@nemeurope.eu>",
    "Guillem <guillem@nemeurope.eu>",
    "Thomas Tran <tldinteractive@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/proximax-storage/proximax-nem2-sdk-ts-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/proximax-storage/proximax-nem2-sdk-ts-js.git"
  },
  "publishConfig": {
    "registry": "https://nexus.internal.proximax.io/repository/npm-private/"
  },
  "license": "Apache-2.0",
  "homepage": "https://github.com/proximax-storage/proximax-nem2-sdk-ts-jst#readme",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/lodash": "^4.14.85",
    "@types/mocha": "^2.2.44",
    "@types/request": "^2.47.0",
    "@types/request-promise-native": "^1.0.14",
    "@types/utf8": "^2.1.6",
    "@types/ws": "^3.2.0",
    "assert": "^1.4.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "ts-mockito": "^2.2.7",
    "ts-node": "^5.0.1",
    "tslint": "^5.8.0",
    "typescript": "^2.5.3",
    "typescript-require": "^0.2.9-1"
  },
  "dependencies": {
    "js-joda": "^1.6.2",
    "proximax-nem2-library": "^0.9.8",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "rxjs": "^6.2.1",
    "rxjs-compat": "^6.2.1",
    "utf8": "^2.1.2",
    "ws": "^5.2.0"
  },
  "peerDependencies": {
    "js-joda": "^1.6.2",
    "utf8": "^2.1.2",
    "rxjs": "^6.2.1"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "nem2",
    "sdk"
  ],
  "author": "NEM"
}
